{
	"version": 2,
	"builds": [
		{
			"src": "JetBoom/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.12"
			}
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "JetBoom/wsgi.py"
		}
	],
	"env": {
		"DJANGO_SETTINGS_MODULE": "JetBoom.settings",
		"PYTHONUNBUFFERED": "1"
	}
}
